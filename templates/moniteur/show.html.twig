{% extends 'base.html.twig' %}

{% block title %}Moniteur{% endblock %}

{% block body %}
    <h1>Moniteur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ moniteur.id }}</td>
            </tr>
            <tr>
                <th>Nomutilisateur</th>
                <td>{{ moniteur.nomutilisateur }}</td>
            </tr>
            <tr>
                <th>Motdepasse</th>
                <td>{{ moniteur.motdepasse }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ moniteur.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ moniteur.prenom }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ moniteur.sexe }}</td>
            </tr>
            <tr>
                <th>Datedenaissance</th>
                <td>{{ moniteur.datedenaissance ? moniteur.datedenaissance|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ moniteur.adresse }}</td>
            </tr>
            <tr>
                <th>Codepostal</th>
                <td>{{ moniteur.codepostal }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ moniteur.ville }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ moniteur.telephone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_moniteur_index') }}">back to list</a>

    <a href="{{ path('app_moniteur_edit', {'id': moniteur.id}) }}">edit</a>

    {{ include('moniteur/_delete_form.html.twig') }}
{% endblock %}
